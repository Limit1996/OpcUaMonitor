{
  "Logging": {
    "PathFormat": "Logs/log-{Date}.txt",
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "Microsoft.EntityFrameworkCore": "Warning"
    },
    "Json": false,
    "FileSizeLimitBytes": 10485760,
    "RetainedFileCountLimit": 31
  },
  "AllowedHosts": "*"
}
